NODE_ENV=development
HOST=0.0.0.0
PORT=2996

# If DATABASE_URL is set, PG* vars are optional.
# Example: postgres://postgres:password@localhost:5432/cloud_storage
DATABASE_URL=
PGHOST=
PGPORT=
PGUSER=
PGPASSWORD=
PGDATABASE=

# Use a dedicated schema to avoid collisions in shared DBs.
DB_SCHEMA=cloud_storage

SESSION_SECRET=
SHARE_TOKEN_SECRET=

MAX_UPLOAD_BYTES=104857600

# Seed users (run: npm run db:seed)
SUPER_ADMIN_USERNAME=
SUPER_ADMIN_PASSWORD=
SUPER_ADMIN_EMAIL=

ADMIN_USERNAME=
ADMIN_PASSWORD=
ADMIN_EMAIL=

